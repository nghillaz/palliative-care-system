<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.cse360.mobileapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Login"
            android:label="@string/title_activity_login" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateAccount"
            android:label="@string/title_activity_create_account"
            android:parentActivityName=".Login" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.android.cse360.mobileapp.Login" />
        </activity>
        <activity
            android:name=".ForgotPasswordEmail"
            android:label="@string/title_activity_forgot_password_email"
            android:parentActivityName=".Login" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.android.cse360.mobileapp.Login" />
        </activity>
        <activity
            android:name=".MainMenu"
            android:label="@string/title_activity_main_menu"
            android:parentActivityName=".Login" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.android.cse360.mobileapp.Login" />
        </activity>
        <activity
            android:name=".SetDoctor"
            android:label="@string/title_activity_set_doctor"
            android:parentActivityName=".MainMenu" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.android.cse360.mobileapp.MainMenu" />
        </activity>
        <activity
            android:name=".EnterSymptoms"
            android:label="@string/title_activity_enter_symptoms" >
        </activity>
        <activity
            android:name=".EditPersonalDetails"
            android:label="@string/title_activity_edit_personal_details" >
        </activity>
    </application>

</manifest>
